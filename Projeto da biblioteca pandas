      "source": [
        "alunos.info"
      ],
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/",
          "height": 204
        },
        "id": "0xact6bmabOX",
        "outputId": "372143de-c692-4c1c-c6f7-dee6dbc377a9"
